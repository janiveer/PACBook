<TEI xmlns="http://www.tei-c.org/ns/1.0" version="5.0">
  <!-- Morphological Data for Parametric Transclusion -->
  <text>
    <body>
      <head>Italiano</head>
      <entry n="e">
        <note>Usage 'S' indicates form used before an initial impure consonant</note>
        <form>
          <gramGrp>
            <usg value="C S"/>
          </gramGrp>
          <orth>e</orth>
        </form>
        <form>
          <gramGrp>
            <usg value="V"/>
          </gramGrp>
          <orth>ed</orth>
        </form>
      </entry>
      <entry n="o">
        <note>Usage 'S' indicates form used before an initial impure consonant</note>
        <form>
          <gramGrp>
            <usg value="C S"/>
          </gramGrp>
          <orth>o</orth>
        </form>
        <form>
          <gramGrp>
            <usg value="V"/>
          </gramGrp>
          <orth>od</orth>
        </form>
      </entry>
      <entry n="il">
        <note>For the singular form, there must be no space between the dependent word and the following word in the source document.</note>
        <form>
          <gramGrp>
            <usg value="C"/><gen value="m"/><number value="sg"/>
          </gramGrp>
          <orth>il </orth>
        </form>
        <form>
          <gramGrp>
            <usg value="S"/><gen value="m"/><number value="sg"/>
          </gramGrp>
          <orth>lo </orth>
        </form>
        <form>
          <gramGrp>
            <usg value="V"/><gen value="m"/><number value="sg"/>
          </gramGrp>
          <orth>l’</orth>
        </form>
        <form>
          <gramGrp>
            <usg value="C S"/><gen value="f"/><number value="sg"/>
          </gramGrp>
          <orth>la </orth>
        </form>
        <form>
          <gramGrp>
            <usg value="V"/><gen value="f"/><number value="sg"/>
          </gramGrp>
          <orth>l’</orth>
        </form>
        <form>
          <gramGrp>
            <usg value="C"/><gen value="m"/><number value="pl"/>
          </gramGrp>
          <orth>i</orth>
        </form>
        <form>
          <gramGrp>
            <usg value="S V"/><gen value="m"/><number value="pl"/>
          </gramGrp>
          <orth>gli</orth>
        </form>
        <form>
          <gramGrp>
            <usg value="C S V"/><gen value="f"/><number value="pl"/>
          </gramGrp>
          <orth>le</orth>
        </form>
      </entry>
    </body>
  </text>
</TEI>
